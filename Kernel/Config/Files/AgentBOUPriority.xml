<otrs_config version="1.0" init="Application">
    <ConfigItem Name="Frontend::Output::FilterElementPost###AgentBOUPriority" Required="0" Valid="1">
        <Description Translatable="1">Module to show ticket attachments.</Description>
        <Group>BeOnUP Easy Categorization</Group>
        <SubGroup>OutputFilter</SubGroup>
        <Setting>
            <Hash>
                <Item Key="Module">Kernel::Output::HTML::OutputFilter::AgentBOUPriority</Item>
